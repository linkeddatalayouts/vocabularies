@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix lidl: <https://linkeddatalayouts.github.io/vocabularies/lidl.ttl#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix exif: <http://www.w3.org/2003/12/exif/ns> .
@prefix img: <http://www.example.org/2001/image/ns#> .


##################################################
# Layout Instance
##################################################

<cuteKitty> 
  a img:RGBImage ;
  exif:width 3 ;
  exif:height 5 ;
  img:pixels [
    # auto-generated since defined as distributable
    a img:RGBPixelBuffer, dcat:Distribution ;
    dcat:downloadURL "cuteKitty#pixels"^^xsd:anyURI ;
    dct:format "application/octet-stream" ;
    dcat:byteSize 45 
  ] .